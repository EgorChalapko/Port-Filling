        <PAGEOBJECT XPOS="115.354330708661" YPOS="68.5348837209302" OwnPage="0" ItemID="172304992" PTYPE="4" WIDTH="286.622413477385" HEIGHT="174.976744186047" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L286.622 0 L286.622 174.977 L0 174.977 L0 0 Z" copath="M0 0 L286.622 0 L286.622 174.977 L0 174.977 L0 0 Z" gXpos="115.354330708661" gYpos="68.5348837209302" gWidth="0" gHeight="0" PSTYLE="Default Paragraph Style" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
